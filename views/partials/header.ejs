    <!-- Plugin css for this page -->
    <link rel="stylesheet" href="/asserts/vendors/jvectormap/jquery-jvectormap.css">
    <link rel="stylesheet" href="/asserts/vendors/flag-icon-css/css/flag-icon.min.css">
    <link rel="stylesheet" href="/asserts/vendors/owl-carousel-2/owl.carousel.min.css">
    <link rel="stylesheet" href="/asserts/vendors/owl-carousel-2/owl.theme.default.min.css">
    <!-- End plugin css for this page -->
    <!-- inject:css -->
