  <body>
    <% if(locals.admin ? admin : false ){ %>
    <div class="container-scroller">
      <%- include('../partials/sidebar.ejs') %>
      <div class="container-fluid page-body-wrapper dynamic_content">
        <%- include('../partials/navbar.ejs') %>
        <div class="main-panel">
          <div class="content-wrapper" id="dynamic_page">
    <% } %>
    
    