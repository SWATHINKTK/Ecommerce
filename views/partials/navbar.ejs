                  <a href="<%= locals.login ? '/logout' : '/login'  %>"><%= locals.login ? '' : 'Sign in'  %></a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="container-fluid fluid-style">
        <div class="row">
          <div class="col-lg-3 col-md-3">
            <div class="header__logo">
              <a href="<%= locals.login ? '/home' : '/'  %>"><img src="/public/user/img/logo.png" alt="" /></a>
              
            </div>
          </div>
          <div class="col-lg-6 col-md-6">
            <nav class="header__menu mobile-menu">
              <ul>
                <li class="active"><a href="<%= locals.login ? '/home' : '/'  %>">Home</a></li>
                <li><a href="/allproductview?login=<%= locals.login ? true : false  %>">Shop</a></li>
                <!-- <li>
                  <a href="#">Pages</a>
                  <ul class="dropdown">
                    <li><a href="./about.html">About Us</a></li>
                    <li><a href="./shop-details.html">Shop Details</a></li>
                    <li><a href="./shopping-cart.html">Shopping Cart</a></li>
                    <li><a href="./checkout.html">Check Out</a></li>
                    <li><a href="./blog-details.html">Blog Details</a></li>
                  </ul>
                </li> -->
                <li><a href="/about"> About</a></li>
                <li><a href="/contact">Contacts</a></li>
              </ul>
            </nav>
          </div>
          <div class="col-lg-3 col-md-3">
            <div class="header__nav__option">
              <!-- <a href="#" class="search-switch"><img src="/public/user/img/icon/search.png" alt="" /></a> -->
              <a href="/api/viewWishlist"><i class="fa-regular fa-heart fa-lg text-dark"></i></a>
              <a href="/api/cart"><i class="bi bi-cart3 fa-lg text-dark"></i><span id="Cart-Count" class="badge badge-danger" style="margin-top: -17px; margin-left: 10px; border-radius: 50%; font-weight: 900;"><%= locals.cartCount %></span></a>
              <a href="/userprofile"><i class="bi bi-person fa-xl text-dark"></i></a>
                
            </div>
          </div>
        </div>
        <div class="canvas__open"><i class="fa fa-bars"></i></div>
      </div>
    </header>
    <!-- Header Section End -->

    <% } %>