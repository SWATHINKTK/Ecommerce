      <div class="offcanvas__text mb-4">
        <p class="font-weight-bold mb-2">Free shipping, 30-day return or refund guarantee.</p>
      </div>
        <div class="offcanvas__nav__option">
          <a href="/api/viewWishlist"><i class="fa-regular fa-heart fa-lg text-dark"></i></a>
          <a href="/api/cart"><i class="bi bi-cart3 fa-lg text-dark"></i><span id="Cart-Count" class="badge badge-danger" style="margin-top: -17px; margin-left: 10px; border-radius: 50%; font-weight: 900;"><%= locals.cartCount %></span></a>
          <a href="/userprofile"><i class="bi bi-person fa-xl text-dark"></i></a>
        </div>
        <div id="mobile-menu-wrap"></div>
        <div class="offcanvas__option">
          <div class="offcanvas__links">
              <a href="<%= locals.login ? '/logout' : '/login'  %>" class="font-weight-bold"><%= locals.login ? 'Log Out' : 'Sign in'  %></a>
          </div>
