<h1 class="card-title text-center text-dark">Edit Coupon</h1>
   <div class="col-12 grid-margin stretch-card">
      <div class="card">
        <div class="card-body form-style d-flex justify-content-center ">
          <form class="forms-sample mt-3 mb-3 w-50" id="editCouponForm" method="POST">
            <input type="hidden" value="<%= couponData._id %>" id="couponId">
            <div class="" role="alert" id="coupon-submit-result">
            </div>
            <div class="form-group">
              <label for="exampleInputName1">Coupon Name</label>
              <input type="text" class="form-control" id="couponName" placeholder="coupon name" name="couponName" value="<%= couponData.couponName %>">
              <p class="text-danger ml-2" name="validate-coupon"></p>
            </div>
            <div class="form-group">
              <label for="exampleInputName1">Minimum Purchase</label>
              <input type="number" class="form-control" id="minimumPurchase" placeholder="minimum purchase" name="minimumPurchase" value="<%= couponData.minimumPurchase %>">
              <p class="text-danger ml-2" name="validate-coupon"></p>
            </div>
            <div class="form-group">
              <label for="exampleInputName1">Offer Percentage</label>
              <input type="number" class="form-control" id="OfferPercentage" placeholder="offer percentage" name="OfferPercentage" min="1" max="99" value="<%= couponData.OfferPercentage %>">
              <p class="text-danger ml-2" name="validate-coupon"></p>
            </div>
            <div class="form-group">
              <label for="exampleInputName1">Start Date</label>
              <input type="date" class="form-control" id="startDate" placeholder="coupon start date" name="couponStartDate" value="<%= startDate %>">
              <p class="text-danger ml-2" name="validate-coupon"></p>
            </div>
            <div class="form-group">
              <label for="exampleInputName1">End Date</label>
              <input type="date" class="form-control" id="endDate" placeholder="coupon end date" name="couponEndDate" value="<%= endDate %>">
              <p class="text-danger ml-2" name="validate-coupon"></p>
            </div>
            
            <button type="submit" class="btn btn-primary mr-2">Submit</button>
            <button type="submit" class="btn btn-dark mr-2">Cancel</button>
          </form>
        </div>

        
      </div>
      </div>
    </div>   
    