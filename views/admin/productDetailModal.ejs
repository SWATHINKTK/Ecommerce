<div>
  Images:
  <div class="modal-productimage">
    <% for(let i = 0; i< modaldata.productImages.length ;i++) {%>
      <div class="image-modal"><img class="modal-image" src="/public/admin/assets/productImages/<%= modaldata.productImages[i]%>" alt=""></div>
    <% }%>
    <!-- <div class="image-modal"><img class="modal-image" src="/public/admin/assets/productImages/<%= modaldata.productImages[1]%>" alt=""></div>
    <div class="image-modal"><img class="modal-image" src="/public/admin/assets/productImages/<%= modaldata.productImages[2]%>" alt=""></div>
    <div class="image-modal"><img class="modal-image" src="/public/admin/assets/productImages/<%= modaldata.productImages[3]%>" alt=""></div> -->
  </div>
  <div class="product-description">
    Description:<br><span ><%= modaldata.description %></span>
  </div>
  <div class="product-Data">
    Product Added Date : <%= modaldata.addDate %>
  </div> 
  <div class="product-Data">
    Rating : <%= modaldata.rating %>
  </div>
  <div class="product-Data">
    Feedback : <%= modaldata.feedback %>
  </div>
  <div class="product-Data d-flex">
    color : <div id="color-view" style="height: 25px; width: 25px;display: inline-block; border-radius: 50%;margin-left: 7px; " data-product-color="<%= modaldata.color %>"></div>
  </div> 
  <div class="product-Data">
    Material : <%= modaldata.material %>
  </div> 
  <div class="product-Data">
    size : <%= modaldata.size %>
  </div> 
  <div class="product-Data">
    specification : <%= modaldata.specifications %>
  </div> 
  </div>
  <div class="modal-footer">
