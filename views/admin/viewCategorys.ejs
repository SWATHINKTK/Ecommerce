<div class="col-12 grid-margin stretch-card">
  <div class="card">
    <div class="card-body  mt-5">
      <h4 class="card-title text-center">Category View &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</h4>
      </p>
      <div class="table-responsive">
        <table class="table table-hover">
          <thead>
            <tr>
              <th>#</th>
              <th>Category Name</th>
              <th>Edit</th>
              <th>List/Unlist</th>
              <!-- <th>Delete/Retrieve</th> -->
              <!-- <td class="text-danger"> 28.76% <i class="mdi mdi-arrow-down"></i></td>
              <td><label class="badge badge-danger">Pending</label></td> -->
            </tr>
          </thead>
          <tbody class="text-white">
            <% if(data.length > 0) { %>
              <% for(let i=0;i<data.length;i++) { %>
              <tr>
                <td><%= i+1 %></td>
                <td><%= data[i].categoryname %></td>
                <td><button class="btn btn-transparent" onclick="editCategory(this)"><i class="bi bi-pencil-square"></i>Edit</button></td>
                <% if(data[i].list) {%>
                  <td id="<%= data[i]._id %>"><button class="btn btn-warning pl-3 pr-3" id="<%= data[i]._id %>" onclick="categoryList(this)"><i class="bi bi-x-circle"></i>Unlist</button></td>
                <% } else { %>
                  <td id="<%= data[i]._id %>"><button class="btn btn-primary pl-4 pr-4" id="<%= data[i]._id %>" onclick="categoryList(this)"><i class="bi bi-check2-circle"> </i>list</button></td>
                <% } %>

                <!-- <% if(!(data[i].delete) ) {%>
                  <td><button class="btn btn-danger"><i class="bi bi-trash3"></i>Delete</button></label></td>
                <% } else { %>
                  <td><button class="btn btn-danger"><i class="bi bi-plus-square-dotted"></i>Retrieve</button></label></td>
                <% } %> -->

              </tr>
              <% } } else{ %>
                <tr class="bg-danger">aq
                  <th class="text-dark" colspan="5">DATA IS NOT FOUND</th>
                </tr> 
              <% } %>
          </tbody>
        </table>
        </div>
      </div>
    </div>
  </div>
</div>


  <div class="modal" id="modal" style="display: none; width: 23rem;margin-right: 50%;margin-left: 40%; margin-top:3rem;">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="staticBackdropLabel">Category List/Unlist</h5>
        <button class="btn btn-transparent" onclick="back()"><i class="bi bi-x"></i></button>
      </div>
      <div class="modal-body">
        Are you sure?
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" id="cancel" onclick="back()">Cancel</button>
        <button type="button" class="btn btn-primary" id="sucess" data-category-id="true" data-category-name="">OK</button>
      </div>
    </div>
  </div>
</div>